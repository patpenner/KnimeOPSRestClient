<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?eclipse version="3.0"?>
<plugin>

   <!-- This is an example for providing an own category 
      <extension
         point="org.knime.workbench.repository.categories">
      <category
            description="Category containing my nodes"
            icon="icons/default.png"
            level-id="myNodes"
            name="My nodes"
            path="/"/>
   </extension>
   -->
   
  <extension point="org.knime.workbench.repository.nodes">
    <node category-path="/" factory-class="node.org.ops.connector.ConnectorNodeFactory" id="org.ops.connector.ConnectorNodeFactory"/>
    <node category-path="/" factory-class="node.urifinder.URIFinderNodeFactory" id="node.urifinder.URIFinderNodeFactory"/>
    <node category-path="/" factory-class="node.patentsplitter.PatentSplitterNodeFactory" id="node.urifinder.PatentSplitterNodeFactory"/>
   	<node category-path="/" factory-class="node.openPhactsPatent.OpenPhactsPatentNodeFactory" id="node.openPhactsPatent.OpenPhactsPatentNodeFactory"/>
	<node category-path="/" factory-class="node.patent.PatentNodeFactory" id="node.localPatent.PatentNodeFactory"/>
  </extension>
   
</plugin>
